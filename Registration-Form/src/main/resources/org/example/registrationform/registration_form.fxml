<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.registrationform.RegistrationFormController">
   <children>
      <Label layoutX="141.0" layoutY="30.0" text="Registration Form (FXML)">
         <font>
            <Font size="25.0" />
         </font>
      </Label>
       <Label fx:id="welcomeText" layoutX="141.0" layoutY="100.0" text=""/>
       <GridPane id="submit" layoutX="-1.0" prefHeight="400.0" prefWidth="600.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="Name :" GridPane.rowIndex="1" />
            <Label text="Email ID :" GridPane.rowIndex="2" />
            <Label text="Password :" GridPane.rowIndex="3" />
            <TextField id="name" fx:id="name" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <TextField id="email" fx:id="email" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <PasswordField id="password" fx:id="password" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            <Button fx:id="submit" mnemonicParsing="false" onAction="#handleSubmitButtonAction" text="submit" GridPane.rowIndex="4" />
         </children>
      </GridPane>
   </children>
</AnchorPane>
