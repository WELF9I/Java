<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="768.0" prefWidth="1111.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.tp3javafx.AddBookController">
   <children>
      <VBox prefHeight="768.0" prefWidth="1161.0">
         <children>
            <GridPane prefHeight="188.0" prefWidth="1111.0">
              <columnConstraints>
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="451.0" minWidth="10.0" prefWidth="277.0" />
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="717.0" minWidth="10.0" prefWidth="481.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="327.0" minWidth="10.0" prefWidth="327.0" />
              </columnConstraints>
              <rowConstraints>
                  <RowConstraints maxHeight="214.0" minHeight="0.0" prefHeight="50.0" vgrow="SOMETIMES" />
                  <RowConstraints maxHeight="214.0" minHeight="0.0" prefHeight="44.0" vgrow="SOMETIMES" />
                  <RowConstraints maxHeight="282.0" minHeight="0.0" prefHeight="47.0" vgrow="SOMETIMES" />
                <RowConstraints maxHeight="389.0" minHeight="10.0" prefHeight="76.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Label text="Prénom">
                     <GridPane.margin>
                        <Insets bottom="20.0" top="20.0" />
                     </GridPane.margin>
                     <padding>
                        <Insets left="20.0" />
                     </padding>
                     <font>
                        <Font size="15.0" />
                     </font>
                  </Label>
                  <TextField fx:id="tfFirstName" GridPane.columnIndex="1" />
                  <Label text="Nom" GridPane.rowIndex="1">
                     <GridPane.margin>
                        <Insets left="20.0" />
                     </GridPane.margin>
                     <font>
                        <Font size="15.0" />
                     </font>
                  </Label>
                  <TextField fx:id="tfLastName" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <TextField fx:id="tfEmail" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                  <Label text="Adress Email" GridPane.rowIndex="2">
                     <font>
                        <Font size="15.0" />
                     </font>
                     <GridPane.margin>
                        <Insets left="20.0" />
                     </GridPane.margin>
                  </Label>
                  <Button fx:id="addBtn" mnemonicParsing="false" onAction="#handleAddButtonAction" text="Ajouter à la table" GridPane.columnIndex="1" GridPane.rowIndex="3">
                  <GridPane.margin>
                        <Insets left="150.0" />
                     </GridPane.margin>
                     <font>
                        <Font size="17.0" />
                     </font>
                  </Button>
               </children>
            </GridPane>
            <TableView fx:id="table" prefHeight="448.0" prefWidth="1161.0">
              <columns>
                <TableColumn fx:id="firstNameCol" prefWidth="377.0" text="Prénom" />
                <TableColumn fx:id="lastNameCol" prefWidth="388.0" text="Nom" />
                  <TableColumn fx:id="emailCol" prefWidth="391.0" text="Email" />
              </columns>
            </TableView>
            <HBox prefHeight="136.0" prefWidth="1161.0">
               <children>
                  <Button fx:id="removeBtn" mnemonicParsing="false" onAction="#handleRemoveButtonAction" text="Supprimer">
                  <font>
                        <Font size="18.0" />
                     </font>
                     <HBox.margin>
                        <Insets left="150.0" top="50.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="importBtn" mnemonicParsing="false" text="Importer">
                     <font>
                        <Font size="18.0" />
                     </font>
                     <HBox.margin>
                        <Insets left="150.0" top="50.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="exportBtn" mnemonicParsing="false" text="Exporter">
                     <font>
                        <Font size="18.0" />
                     </font>
                     <HBox.margin>
                        <Insets left="150.0" top="50.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="quitBtn" onAction="#handleQuitButtonAction" mnemonicParsing="false" text="Quitter">
                     <font>
                        <Font size="18.0" />
                     </font>
                     <HBox.margin>
                        <Insets left="150.0" top="50.0" />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
